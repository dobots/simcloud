<model name="human">
    <static>true</static>
   <pose>5.4 3.5 0.8 0 3.14 -1.57</pose>
    <link name="body"> 
   <visual name="visual">
        <geometry>
          <mesh>
            <uri>model://human_resting_description/meshes/man_resting.dae</uri>
          </mesh>
        </geometry>
        <cast_shadows>true</cast_shadows>
      </visual>
  <collision name="collision">
       <geometry>
          <mesh>
            <uri>model://human_resting_description/meshes/man_resting.dae</uri>
          </mesh>
        </geometry>
      </collision>
    </link>
  </model>



